{
    "manifest_version": 3,
    "name": "Quick Notes",
    "version": "1.0",
    "description": "A simple note-taking Chrome extension.",
    "action": {
      "default_popup": "entry.html",
      "default_icon": "dev_diary.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage"]
  }
  